	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main",
		"lint": "eslint \"src/**/*.ts\" --fix",
		"dev": "yarn start:dev",
		"prepare": "husky install",
		"test": "jest ./src/test/main.test.ts --rootDir=./",
		"test:watch": "jest --config ./jest.config.ts --watch",
		"test:cov": "jest --coverage",
		"test:e2e": "jest --config ./jest.config.ts",
		"test:e2e:watch": "jest --config ./jest.config.ts --watch",
		"migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/database/config.ts",
		"migration:generate": "typeorm migration:create",
		"schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop -d src/database/config.ts"
	},
